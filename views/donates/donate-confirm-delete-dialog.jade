#confirm-delete.modal.fade.small(tabindex="-1", role="dialog", aria-labelledby="confirm-delete-title", aria-hidden="true")
    .modal-dialog
        .modal-content
            .modal-header
                button.close(type="button", data-dismiss="modal", aria-hidden="true") &times;
                h4#confirm-delete-title.modal-title Подтверждение удаления
            .modal-body
                p Вы желаете произвести удаление этой операции?
            .modal-footer
                +aButtonCancel("oper-del-cancel-button")(data-dismiss="modal")
                +aButtonDanger("oper-del-conf-button","Удалить")
